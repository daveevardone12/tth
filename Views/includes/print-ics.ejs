            margin-bottom: 3em;
        }

        .signature-box  .sig{
            text-align: start;
            margin-bottom: 3em;
        }

        .signature-box span {
            display: block;
            margin-top: 15px;
        }
      
        .reminder {
            font-size: 12px;
            text-align: justify;
        }

        .small-note {
            font-size: 10px;
            text-align: center;
            margin-top: 5px;
        }

        .tbady .t-ar .t-d {
            padding: 3em;
